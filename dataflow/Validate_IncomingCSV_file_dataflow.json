{
	"name": "Validate_IncomingCSV_file_dataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_source_emp2",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable3",
						"type": "DatasetReference"
					},
					"name": "sink1"
				},
				{
					"dataset": {
						"referenceName": "DelimitedText18",
						"type": "DatasetReference"
					},
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "assert1"
				},
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "filter2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          emp_id as string,",
				"          emp_name as string,",
				"          dept as string,",
				"          salary as short",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 assert(expectTrue(!isNull(toInteger(emp_id)), false, 'assertEmpid', null, 'Only integer value allowed')) ~> assert1",
				"derivedColumn1 filter(IsError == false()) ~> filter1",
				"assert1 derive(IsError = isError(),",
				"          ErrorMsg = assertErrorMessages()) ~> derivedColumn1",
				"derivedColumn1 filter(IsError == true()) ~> filter2",
				"filter1 sink(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     input(",
				"          emp_id as string,",
				"          emp_name as string,",
				"          dept as string,",
				"          salary as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          emp_id,",
				"          emp_name,",
				"          dept,",
				"          salary",
				"     ),",
				"     preCommands: [],",
				"     postCommands: []) ~> sink1",
				"filter2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink2"
			]
		}
	}
}